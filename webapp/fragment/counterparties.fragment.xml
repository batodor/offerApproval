		<HBox justifyContent="SpaceBetween" width="100%">
			<VBox class="leftVBoxStyle" width="50%">
				<Label text="Partner Name"></Label>
				<Text text="Datang Overseas (Beijing)" />
			</VBox>
			<VBox class="rightVBoxStyle" width="50%">
				<Label text="Rating"></Label>
				<Text text="B" />
			</VBox>
		</HBox>
		<HBox justifyContent="SpaceBetween" width="100%">
			<VBox class="leftVBoxStyle" width="50%">
				<Label class="topLabel" text="Blacklist Status"></Label>
